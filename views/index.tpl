<script src="static/js/jquery.windy.js"></script>
<script>	
$(function() {

    var curslide = 1;

    var $el = $( '#wi-el' ),
        windy = $el.windy(),
        allownavnext = false,
        allownavprev = false;

    $( '#nav-prev' ).on( 'mousedown', function( event ) {
        navprev();
    });

    $( '#nav-next' ).on( 'mousedown', function( event ) {
        navnext();
    });

    function navnext() {
        cropmode = false;
        $('#rect_'+curslide).css('display', 'none');
        if( curslide < 4 ) 
        {
            curslide++;
            _curslide = curslide;
// console.log(curslide, _curslide)            ;
            destroyCropMode( curslide );
            $('.left_notes').hide(); 
            $('#left_notes' + curslide).show(); 
            $('.bottom-title').hide(); 
            $('#bottom-title' + curslide).show(); 
        }
        // else{
        //   return;
        // }
        windy.next(); 
        if( $('#rect_'+_curslide).css('left') != 'auto' ){
          window.setTimeout(function(){
            $('#rect_'+_curslide).css('display', 'block');
          }, 500);
        }
    }

    function navprev() {
        cropmode = false;
        $('#rect_'+curslide).css('display', 'none');
        if( curslide > 1 ) 
        {
            curslide--;
            _curslide = curslide;
            destroyCropMode( curslide );
            $('.left_notes').hide(); 
            $('#left_notes' + curslide).show(); 
            $('.bottom-title').hide(); 
            $('#bottom-title' + curslide).show(); 
        } 
        // else{
        //   return;
        // }
        windy.prev();
        if( $('#rect_'+_curslide).css('left') != 'auto' ){
          window.setTimeout(function(){
            $('#rect_'+_curslide).css('display', 'block');
          }, 500);
          
        }
         
    }

});
</script>

