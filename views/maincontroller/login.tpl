<script>
  $(document).ready(function(){
    $('#postForm').submit(function(e){
      e.preventDefault();
      var email = $("#user-email").val();
      var password = $("#user-password").val();
      var severName = "http://devel.medlex.ai:8080";
      var url = severName + '/api/login';

      $.ajax({
            url: url,
            type: 'post',
            dataType: 'json',
            contentType: 'application/json',
            data: JSON.stringify({email:email, password:password})
        })
      .done(function(data){
        if ('error' in data) {
          alert('Error happens:' + data.error)
        } else {
            window.location.href = severName+ "/allcharts";
        }
      })
      .fail(function(jqXHR, textStatus){
        $(".error_messages").html("error");
      });
      return false;
    });
  });
</script>
  
